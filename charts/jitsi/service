# Source: jitsi-meet/templates/jicofo/deployment.yaml
apiVersion: apps/v1
kind: Deployment
metadata:
  name: RELEASE-NAME-jitsi-meet-jicofo
  labels:
    helm.sh/chart: jitsi-meet-0.1.1
    app.kubernetes.io/name: jitsi-meet
    app.kubernetes.io/instance: RELEASE-NAME
    app.kubernetes.io/version: "4416"
    app.kubernetes.io/managed-by: Helm
    app.kubernetes.io/component: jicofo
spec:
  replicas: 1
  selector:
    matchLabels:
      app.kubernetes.io/name: jitsi-meet
      app.kubernetes.io/instance: RELEASE-NAME
      app.kubernetes.io/component: jicofo
  template:
    metadata:
      labels:
        app.kubernetes.io/name: jitsi-meet
        app.kubernetes.io/instance: RELEASE-NAME
        app.kubernetes.io/component: jicofo
      annotations:
        checksum/config: 4739ff6d45febac559a3a7b0d01da5e1ba3b6a6bb83fc775e8de6b110c68a9e9
        checksum/secret: a0eaf5619e05ac1a5378a37f1e86109215c900060f4f89627b807b364f9967b9
    spec:
      serviceAccountName: RELEASE-NAME-jitsi-meet
      securityContext:
        {}
      containers:
        - name: jitsi-meet
          securityContext:
            {}
          image: "jitsi/jicofo:4416"
          imagePullPolicy: IfNotPresent
          envFrom:
          - secretRef:
              name: RELEASE-NAME-prosody-jicofo
          - configMapRef:
              name: RELEASE-NAME-prosody-common
          - configMapRef:
              name: RELEASE-NAME-jitsi-meet-jicofo
          ports:
            - name: http
              containerPort: 80
              protocol: TCP
          livenessProbe:
            tcpSocket:
              port: 8888
          readinessProbe:
            tcpSocket:
              port: 8888
          resources:
            {}
---
